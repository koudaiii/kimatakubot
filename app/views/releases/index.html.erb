<h1>Listing releases</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th>Release at</th>
    </tr>
  </thead>

  <tbody>
    <% @releases.each do |release| %>
      <tr>
        <td><%= release.title %></td>
        <td><%= release.release_at %></td>
        <td><%= link_to 'Show', release %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Release', new_release_path %>
